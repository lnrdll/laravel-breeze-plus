{
    "name": "rlunardelli/laravel-breeze-plus",
    "description": "Minimal Laravel Breeze profile scaffolding with Blade and Tailwind.",
    "keywords": [
        "laravel",
        "breeze",
        "breeze-plus"
    ],
    "support": {
        "source": "https://github.com/rlunardelli/laravel-breeze-plus",
        "issues": "https://github.com/rlunardelli/laravel-breeze-plus/issues"
    },
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Robson Lunardelli",
            "email": "robson@l9s.dev"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/breeze": "^1.4",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rlunardelli\\LaravelBreezePlus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rlunardelli\\LaravelBreezePlus\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rlunardelli\\LaravelBreezePlus\\LaravelBreezePlusServiceProvider"
            ],
            "aliases": {
                "LaravelBreezePlus": "Rlunardelli\\LaravelBreezePlus\\LaravelBreezePlusFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
